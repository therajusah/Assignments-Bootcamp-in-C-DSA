                                 // Questions is based on Iterative Control Statements
//1. Write a program to check whether a given number is positive or non-positive.
#include <stdio.h>
int main() {
  int i =1;
  while(i<=5){
    printf("mysiirg\n");
      i++;
  }
  return 0;
}

// 2. Write a program to print the first 10 natural numbers.
#include <stdio.h>
int main(){
    int i;
for(i=1; i<=10; i++){
printf("%d\n", i);
}
return 0;
}

// 3. Write a program to print the first 10 natural numbers in reverse order

#include <stdio.h>
int main(){
  int i;
for(i=1; i<=10; i++){
printf("%d\n", i);
}
return 0;
}

