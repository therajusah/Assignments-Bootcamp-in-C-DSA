// 8. Write a program to check whether a given year is a leap year or not.
#include <stdio.h>
int main(){
int year;
printf("Enter a year to find put it's a leap year or not");
scanf("%d", &year);
if (year%4==0){
printf("%d is leap year", year);
}
else {
printf("%d is not leap year", year);
}
return 0;
}

// 9. Write a program to find the greatest among three given numbers. Print number once
if the greatest number appears two or three times.
#include <stdio.h>
int main(){
int a,b,c;
printf("Enter three number");
scanf("%d %d %d", &a, &b,&c);
if(a>b){
printf("%d is greater than b and c", a);
}
else if (b>c){
printf("%d is greater than c", b);
}else if (c>a){
printf("%d is greater than a", c);
} else
{
printf("values are same");
}
return 0;
}

// 10. Write a program which takes the cost price and selling price of a product from the
user. Now calculate and print profit or loss percentage.

#include <stdio.h>
int main() {
int cp,sp;
printf("Enter selling price");
scanf("%d", &sp);
printf("Enter cost price");
scanf("%d", &cp);
int profit = sp-cp;
int loss = cp-sp;
if (profit>loss){
printf("profit is %d", profit);
}
else if (loss>profit){
printf("loss is %d", loss);
}
else {
printf("No profit no loss");
}
return 0;
}

// [Needs correction in code of this Question] 11. Write a program to take marks of 5 subjects from the user. Assume marks are given
out of 100 and passing marks is 33. Now display whether the candidate passed the
examination or failed.
#include <stdio.h>
int main() {
int Maths,Chem,Phy,Bio,Eng;
printf("Enter marks of your Maths");
scanf("%d", &Maths);
printf("Enter marks of your Chemistry");
scanf("%d", &Chem);
  printf("Enter marks of your Physics");
scanf("%d", &Phy);
printf("Enter marks of your Biology");
scanf("%d", &Bio);
printf("Enter marks of your English");
scanf("%d", &Eng);
  if(Maths<33){
    printf("Student is fail in Maths");
  }
  else if(Chem<33){
    printf("Student is fail in Chemistry");
  }
  else if(Bio<33){
    printf("Student is fail in Biology");
  }
  else if(Eng<33){
    printf("Student is fail in English");
  }
 else if(Maths,Chem,Phy,Bio,Eng>=33){
  printf("Student is pass");
}
  else if(Maths,Chem,Phy,Bio,Eng<33)
  {
    printf("Student is fail in all 5 Subjects");
  }
return 0;
  }
  
  // 12. Write a program to check whether a given alphabet is in uppercase or lowercase.

#include <stdio.h>
int main(){
  char ch;
  printf("Enter a character to find out it's small or Capital");
  scanf("%c", &ch);
if(ch>='A' && ch<='Z'){
  printf("%c is Capital", ch);
}
  else{
    printf("%c is small");
  }
  return 0;
}

// 13. Write a program to check whether a given number is divisible by 3 and divisible by 2.

#include <stdio.h>
int main(){
  int a;
  printf("Enter a Number");
  scanf("%d", &a);
  if (a%3==0 && a%2==0){
    printf("%d is divisible by both 2 and 3", a);
    }
else if(a%2==0){
  printf("%d is divisible by 2", a);
}
  else if (a%3==0){
    printf("%d is divisible by 3", a);
  }
  else if (a%3==0 && a%2==0){
    printf("%d is divisible by both 2 and 3", a);
  }
  else {
    printf("%d is nor divisible by 3 nor by 2");
  }
  return 0;
}

